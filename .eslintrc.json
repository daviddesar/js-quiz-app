{
  // "extends": "eslint:recommended",
  "plugins": ["@typescript-eslint", "prettier"],
  "ignorePatterns": ["**.config.js"],
  "extends": [
    "prettier",
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended"
  ],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "React": true
  }
}
